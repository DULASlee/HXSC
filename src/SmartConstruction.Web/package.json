{
  "name": "smart-construction-babaob-framework-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "api:generate": "npx openapi-typescript http://localhost:7999/swagger/v1/swagger.json --output src/api/generated-types.d.ts",
    "api:generate-local": "npx openapi-typescript ./swagger.json --output src/api/generated-types.d.ts",
    "api:validate": "node -e \"const { apiManager } = require('./dist/api/manager.js'); console.log(apiManager.validateConfig());\""
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@types/three": "^0.178.1",
    "@vueuse/core": "^10.7.0",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "element-plus": "^2.4.4",
    "js-cookie": "^3.0.5",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "pinia": "^2.1.7",
    "terser": "^5.43.1",
    "three": "^0.178.0",
    "vue": "^3.3.11",
    "vue-i18n": "^11.1.10",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
